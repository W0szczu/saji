const grid = [];
const totalCells = 64;
let counter = 0;
let row = [];

for (let i = 0; i <= totalCells; i++) {
    if (i % 8 === 0) {
        if (row.length > 0) {
            grid.push(row);
        }
        row = [];
    }
    
    if (i < totalCells) {
        row.push(counter);
    }
    
    counter++;
}

console.table(grid);


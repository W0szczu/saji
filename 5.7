
let obiekt = {
  imie: "Jan",
  wiek: 25,
  miasto: "Warszawa"
};


console.log("Iteracja przez obiekt:");
for (let klucz in obiekt) {
  if (obiekt.hasOwnProperty(klucz)) {
    console.log(klucz + ": " + obiekt[klucz]);
  }
}


